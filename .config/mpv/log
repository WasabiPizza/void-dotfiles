[1457.234][d][vo/gpu/opengl] [ 14] };
[1457.234][d][vo/gpu/opengl] [ 15] uniform sampler2D lut;
[1457.234][d][vo/gpu/opengl] [ 16] uniform sampler2D dither;
[1457.234][d][vo/gpu/opengl] [ 17] uniform sampler2D texture0;
[1457.234][d][vo/gpu/opengl] [ 18] void main() {
