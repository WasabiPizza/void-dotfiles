[  18.350][d][vo/gpu/opengl] [ 15] void main() {
[  18.350][d][vo/gpu/opengl] [ 16] vec4 color = vec4(0.0, 0.0, 0.0, 1.0);
[  18.350][d][vo/gpu/opengl] [ 17] #undef tex
[  18.350][d][vo/gpu/opengl] [ 18] #undef texmap
[  18.350][d][vo/gpu/opengl] [ 19] #define tex texture0
[  18.350][d][vo/gpu/opengl] [ 20] #define texmap texmap0
[  18.350][d][vo/gpu/opengl] [ 21] vec2 pos = texcoord0;
[  18.350][d][vo/gpu/opengl] [ 22] vec2 size = texture_size0;
[  18.350][d][vo/gpu/opengl] [ 23] vec2 pt = pixel_size0;
[  18.350][d][vo/gpu/opengl] [ 24] // first pass
[  18.350][d][vo/gpu/opengl] [ 25] color = vec4(0.0);
[  18.350][d][vo/gpu/opengl] [ 26] {
[  18.350][d][vo/gpu/opengl] [ 27] vec2 dir = vec2(0.0, 1.0);
[  18.350][d][vo/gpu/opengl] [ 28] pt *= dir;
[  18.350][d][vo/gpu/opengl] [ 29] float fcoord = dot(fract(pos * size - vec2(0.5)), dir);
[  18.350][d][vo/gpu/opengl] [ 30] vec2 base = pos - fcoord * pt - pt * vec2(2.0);
[  18.350][d][vo/gpu/opengl] [ 31] vec4 c;
[  18.350][d][vo/gpu/opengl] [ 32] float ypos = LUT_POS(fcoord, 64.0);
[  18.350][d][vo/gpu/opengl] [ 33] float weights[6];
[  18.350][d][vo/gpu/opengl] [ 34] c = texture(lut, vec2(0.250000, ypos));
[  18.350][d][vo/gpu/opengl] [ 35] weights[0] = c[0];
[  18.350][d][vo/gpu/opengl] [ 36] weights[1] = c[1];
[  18.350][d][vo/gpu/opengl] [ 37] weights[2] = c[2];
[  18.350][d][vo/gpu/opengl] [ 38] weights[3] = c[3];
[  18.350][d][vo/gpu/opengl] [ 39] c = texture(lut, vec2(0.750000, ypos));
[  18.350][d][vo/gpu/opengl] [ 40] weights[4] = c[0];
[  18.350][d][vo/gpu/opengl] [ 41] weights[5] = c[1];
[  18.350][d][vo/gpu/opengl] [ 42] // scaler samples
[  18.350][d][vo/gpu/opengl] [ 43] c = texture(tex, base + pt * vec2(0.0));
[  18.350][d][vo/gpu/opengl] [ 44] color += vec4(weights[0]) * c;
