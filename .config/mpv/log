[  30.455][d][vo/gpu/opengl] [ 12] };
[  30.455][d][vo/gpu/opengl] [ 13] uniform sampler2D lut;
[  30.455][d][vo/gpu/opengl] [ 14] uniform sampler2D texture0;
[  30.455][d][vo/gpu/opengl] [ 15] void main() {
[  30.455][d][vo/gpu/opengl] [ 16] vec4 color = vec4(0.0, 0.0, 0.0, 1.0);
[  30.455][d][vo/gpu/opengl] [ 17] #undef tex
[  30.455][d][vo/gpu/opengl] [ 18] #undef texmap
[  30.455][d][vo/gpu/opengl] [ 19] #define tex texture0
[  30.455][d][vo/gpu/opengl] [ 20] #define texmap texmap0
[  30.455][d][vo/gpu/opengl] [ 21] vec2 pos = texcoord0;
[  30.455][d][vo/gpu/opengl] [ 22] vec2 size = texture_size0;
[  30.455][d][vo/gpu/opengl] [ 23] vec2 pt = pixel_size0;
[  30.455][d][vo/gpu/opengl] [ 24] // first pass
[  30.455][d][vo/gpu/opengl] [ 25] color = vec4(0.0);
[  30.455][d][vo/gpu/opengl] [ 26] {
[  30.455][d][vo/gpu/opengl] [ 27] vec2 dir = vec2(0.0, 1.0);
[  30.455][d][vo/gpu/opengl] [ 28] pt *= dir;
[  30.455][d][vo/gpu/opengl] [ 29] float fcoord = dot(fract(pos * size - vec2(0.5)), dir);
[  30.455][d][vo/gpu/opengl] [ 30] vec2 base = pos - fcoord * pt - pt * vec2(3.0);
[  30.455][d][vo/gpu/opengl] [ 31] vec4 c;
[  30.455][d][vo/gpu/opengl] [ 32] float ypos = LUT_POS(fcoord, 64.0);
[  30.455][d][vo/gpu/opengl] [ 33] float weights[8];
[  30.455][d][vo/gpu/opengl] [ 34] c = texture(lut, vec2(0.250000, ypos));
[  30.455][d][vo/gpu/opengl] [ 35] weights[0] = c[0];
[  30.455][d][vo/gpu/opengl] [ 36] weights[1] = c[1];
[  30.455][d][vo/gpu/opengl] [ 37] weights[2] = c[2];
[  30.455][d][vo/gpu/opengl] [ 38] weights[3] = c[3];
[  30.455][d][vo/gpu/opengl] [ 39] c = texture(lut, vec2(0.750000, ypos));
[  30.455][d][vo/gpu/opengl] [ 40] weights[4] = c[0];
[  30.455][d][vo/gpu/opengl] [ 41] weights[5] = c[1];
[  30.455][d][vo/gpu/opengl] [ 42] weights[6] = c[2];
[  30.455][d][vo/gpu/opengl] [ 43] weights[7] = c[3];
[  30.455][d][vo/gpu/opengl] [ 44] // scaler samples
[  30.455][d][vo/gpu/opengl] [ 45] c = texture(tex, base + pt * vec2(0.0));
[  30.455][d][vo/gpu/opengl] [ 46] color += vec4(weights[0]) * c;
[  30.455][d][vo/gpu/opengl] [ 47] c = texture(tex, base + pt * vec2(1.0));
[  30.455][d][vo/gpu/opengl] [ 48] color += vec4(weights[1]) * c;
[  30.455][d][vo/gpu/opengl] [ 49] c = texture(tex, base + pt * vec2(2.0));
[  30.455][d][vo/gpu/opengl] [ 50] color += vec4(weights[2]) * c;
[  30.455][d][vo/gpu/opengl] [ 51] c = texture(tex, base + pt * vec2(3.0));
[  30.455][d][vo/gpu/opengl] [ 52] color += vec4(weights[3]) * c;
[  30.455][d][vo/gpu/opengl] [ 53] c = texture(tex, base + pt * vec2(4.0));
[  30.455][d][vo/gpu/opengl] [ 54] color += vec4(weights[4]) * c;
[  30.455][d][vo/gpu/opengl] [ 55] c = texture(tex, base + pt * vec2(5.0));
[  30.455][d][vo/gpu/opengl] [ 56] color += vec4(weights[5]) * c;
[  30.455][d][vo/gpu/opengl] [ 57] c = texture(tex, base + pt * vec2(6.0));
[  30.455][d][vo/gpu/opengl] [ 58] color += vec4(weights[6]) * c;
[  30.455][d][vo/gpu/opengl] [ 59] c = texture(tex, base + pt * vec2(7.0));
[  30.456][d][vo/gpu/opengl] [ 60] color += vec4(weights[7]) * c;
[  30.456][d][vo/gpu/opengl] [ 61] }
[  30.456][d][vo/gpu/opengl] [ 62] color *= 1.000000;
[  30.456][d][vo/gpu/opengl] [ 63] out_color = color;
[  30.456][d][vo/gpu/opengl] [ 64] }
