[  30.454][d][vo/gpu/opengl] [  1] #version 440
[  30.454][d][vo/gpu/opengl] [  2] #define tex1D texture
[  30.454][d][vo/gpu/opengl] [  3] #define tex3D texture
[  30.454][d][vo/gpu/opengl] [  4] #define LUT_POS(x, lut_size) mix(0.5 / (lut_size), 1.0 - 0.5 / (lut_size), (x))
[  30.454][d][vo/gpu/opengl] [  5] out vec4 out_color;
[  30.454][d][vo/gpu/opengl] [  6] in vec2 texcoord0;
[  30.454][d][vo/gpu/opengl] [  7] layout(std140, binding=0) uniform UBO {
[  30.455][d][vo/gpu/opengl] [  8] layout(offset=0) vec2 texture_size0;
[  30.455][d][vo/gpu/opengl] [  9] layout(offset=16) mat2 texture_rot0;
