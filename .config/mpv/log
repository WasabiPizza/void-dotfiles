[  18.349][d][vo/gpu/opengl] [ 24] color.r = 1.000000 * vec4(texture(texture0, texcoord0)).r;
[  18.349][d][vo/gpu/opengl] [ 25] color.gb = 1.000000 * vec4(texture(texture1, texcoord1)).rg;
[  18.349][d][vo/gpu/opengl] [ 26] color = color.rgbr;
[  18.349][d][vo/gpu/opengl] [ 27] color.rgb = mat3(colormatrix) * color.rgb + colormatrix_c;
[  18.349][d][vo/gpu/opengl] [ 28] color.a = 1.0;
[  18.349][d][vo/gpu/opengl] [ 29] // scaler pre-conversion
[  18.349][d][vo/gpu/opengl] [ 30] // linearize
[  18.349][d][vo/gpu/opengl] [ 31] color.rgb = clamp(color.rgb, 0.0, 1.0);
[  18.349][d][vo/gpu/opengl] [ 32] color.rgb = pow(color.rgb, vec3(2.4));
[  18.349][d][vo/gpu/opengl] [ 33] color.rgb *= vec3(1.0/1.000000);
[  18.349][d][vo/gpu/opengl] [ 34] // main scaling
[  18.349][d][vo/gpu/opengl] [ 35] out_color = color;
[  18.349][d][vo/gpu/opengl] [ 36] }
