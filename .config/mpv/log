[  18.336][d][vo/gpu/opengl] [  5] out vec4 out_color;
[  18.336][d][vo/gpu/opengl] [  6] in vec2 texcoord0;
[  18.336][d][vo/gpu/opengl] [  7] layout(std140, binding=0) uniform UBO {
[  18.336][d][vo/gpu/opengl] [  8] layout(offset=0) vec2 texture_size0;
[  18.336][d][vo/gpu/opengl] [  9] layout(offset=16) mat2 texture_rot0;
[  18.336][d][vo/gpu/opengl] [ 10] layout(offset=48) vec2 texture_off0;
[  18.336][d][vo/gpu/opengl] [ 11] layout(offset=56) vec2 pixel_size0;
[  18.336][d][vo/gpu/opengl] [ 12] };
[  18.336][d][vo/gpu/opengl] [ 13] uniform float random;
[  18.336][d][vo/gpu/opengl] [ 14] uniform sampler2D texture0;
[  18.336][d][vo/gpu/opengl] [ 15] #define HOOKED_raw texture0
[  18.336][d][vo/gpu/opengl] [ 16] #define HOOKED_pos texcoord0
[  18.336][d][vo/gpu/opengl] [ 17] #define HOOKED_size texture_size0
[  18.336][d][vo/gpu/opengl] [ 18] #define HOOKED_rot texture_rot0
[  18.336][d][vo/gpu/opengl] [ 19] #define HOOKED_off texture_off0
[  18.336][d][vo/gpu/opengl] [ 20] #define HOOKED_pt pixel_size0
[  18.336][d][vo/gpu/opengl] [ 21] #define HOOKED_map texmap0
[  18.336][d][vo/gpu/opengl] [ 22] #define HOOKED_mul 1.000000
[  18.336][d][vo/gpu/opengl] [ 23] #define HOOKED_tex(pos) (HOOKED_mul * vec4(texture(HOOKED_raw, pos)).rgba)
[  18.336][d][vo/gpu/opengl] [ 24] #define HOOKED_texOff(off) HOOKED_tex(HOOKED_pos + HOOKED_pt * vec2(off))
[  18.336][d][vo/gpu/opengl] [ 25] #define CHROMA_raw texture0
[  18.336][d][vo/gpu/opengl] [ 26] #define CHROMA_pos texcoord0
[  18.336][d][vo/gpu/opengl] [ 27] #define CHROMA_size texture_size0
[  18.336][d][vo/gpu/opengl] [ 28] #define CHROMA_rot texture_rot0
[  18.336][d][vo/gpu/opengl] [ 29] #define CHROMA_off texture_off0
[  18.336][d][vo/gpu/opengl] [ 30] #define CHROMA_pt pixel_size0
[  18.336][d][vo/gpu/opengl] [ 31] #define CHROMA_map texmap0
[  18.336][d][vo/gpu/opengl] [ 32] #define CHROMA_mul 1.000000
[  18.336][d][vo/gpu/opengl] [ 33] #define CHROMA_tex(pos) (CHROMA_mul * vec4(texture(CHROMA_raw, pos)).rgba)
[  18.336][d][vo/gpu/opengl] [ 34] #define CHROMA_texOff(off) CHROMA_tex(CHROMA_pos + CHROMA_pt * vec2(off))
[  18.336][d][vo/gpu/opengl] [ 35] float mod289(float x) { return x - floor(x * 1.0/289.0) * 289.0; }
[  18.336][d][vo/gpu/opengl] [ 36] float permute(float x) {
[  18.336][d][vo/gpu/opengl] [ 37] return mod289( mod289(34.0*x + 1.0) * (fract(x) + 1.0) );
[  18.336][d][vo/gpu/opengl] [ 38] }
[  18.336][d][vo/gpu/opengl] [ 39] float rand(float x) { return fract(x * 1.0/41.0); }
[  18.336][d][vo/gpu/opengl] [ 40] vec4 average(float range, inout float h) {
[  18.336][d][vo/gpu/opengl] [ 41] float dist = rand(h) * range; h = permute(h);
[  18.336][d][vo/gpu/opengl] [ 42] float dir = rand(h) * 6.2831853; h = permute(h);
[  18.336][d][vo/gpu/opengl] [ 43] vec2 o = dist * vec2(cos(dir), sin(dir));
[  18.336][d][vo/gpu/opengl] [ 44] vec4 ref[4];
[  18.336][d][vo/gpu/opengl] [ 45] ref[0] = HOOKED_texOff(vec2( o.x, o.y));
[  18.336][d][vo/gpu/opengl] [ 46] ref[1] = HOOKED_texOff(vec2(-o.y, o.x));
[  18.336][d][vo/gpu/opengl] [ 47] ref[2] = HOOKED_texOff(vec2(-o.x, -o.y));
[  18.336][d][vo/gpu/opengl] [ 48] ref[3] = HOOKED_texOff(vec2( o.y, -o.x));
[  18.336][d][vo/gpu/opengl] [ 49] return (ref[0] + ref[1] + ref[2] + ref[3])*0.25;
[  18.336][d][vo/gpu/opengl] [ 50] }
[  18.336][d][vo/gpu/opengl] [ 51] void main() {
[  18.336][d][vo/gpu/opengl] [ 52] vec4 color = vec4(0.0, 0.0, 0.0, 1.0);
[  18.336][d][vo/gpu/opengl] [ 53] {
[  18.336][d][vo/gpu/opengl] [ 54] vec3 _m = vec3(HOOKED_pos, random) + vec3(1.0);
[  18.336][d][vo/gpu/opengl] [ 55] float h = permute(permute(permute(_m.x)+_m.y)+_m.z);
[  18.336][d][vo/gpu/opengl] [ 56] color = HOOKED_tex(HOOKED_pos);
[  18.336][d][vo/gpu/opengl] [ 57] vec4 avg, diff;
[  18.336][d][vo/gpu/opengl] [ 58] avg = average(16.000000, h);
[  18.336][d][vo/gpu/opengl] [ 59] diff = abs(color - avg);
[  18.336][d][vo/gpu/opengl] [ 60] color = mix(avg, color, greaterThan(diff, vec4(0.003906)));
[  18.336][d][vo/gpu/opengl] [ 61] vec3 noise;
[  18.336][d][vo/gpu/opengl] [ 62] noise.x = rand(h); h = permute(h);
[  18.336][d][vo/gpu/opengl] [ 63] noise.y = rand(h); h = permute(h);
[  18.336][d][vo/gpu/opengl] [ 64] noise.z = rand(h); h = permute(h);
[  18.336][d][vo/gpu/opengl] [ 65] color.xyz += 0.005859 * (noise - vec3(0.5));
[  18.336][d][vo/gpu/opengl] [ 66] }
[  18.336][d][vo/gpu/opengl] [ 67] color.b = 0.000000;
[  18.336][d][vo/gpu/opengl] [ 68] color.a = 1.000000;
[  18.336][d][vo/gpu/opengl] [ 69] out_color = color;
[  18.336][d][vo/gpu/opengl] [ 70] }
[  18.336][d][vo/gpu/opengl] shader link log (status=1): 
