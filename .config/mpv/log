[1457.234][d][vo/gpu/opengl] [  4] #define LUT_POS(x, lut_size) mix(0.5 / (lut_size), 1.0 - 0.5 / (lut_size), (x))
[1457.234][d][vo/gpu/opengl] [  5] out vec4 out_color;
[1457.234][d][vo/gpu/opengl] [  6] in vec2 texcoord0;
